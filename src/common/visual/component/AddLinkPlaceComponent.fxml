<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="50.0" prefWidth="850.0" styleClass="component" stylesheets="@DownloadLinkComponent.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="common.controller.component.AddLinkPlaceComponent">
   <children>
      <HBox alignment="CENTER" layoutX="14.0" layoutY="14.0" spacing="15.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <children>
            <Label text="Name:" textFill="WHITE">
               <font>
                  <Font name="System Italic" size="15.0" />
               </font>
            </Label>
            <JFXTextField fx:id="NameTXF" focusColor="#00b2ff" focusTraversable="false" style="-fx-text-fill: white;" unFocusColor="#ffffffa6">
               <font>
                  <Font name="System Italic" size="12.0" />
               </font>
            </JFXTextField>
            <Label text="Description:" textFill="WHITE">
               <font>
                  <Font name="System Italic" size="12.0" />
               </font>
            </Label>
            <JFXTextField fx:id="DescriptionTXF" focusColor="#00b2ff" focusTraversable="false" style="-fx-text-fill: white;" unFocusColor="#ffffffa6">
               <font>
                  <Font name="System Italic" size="12.0" />
               </font>
            </JFXTextField>
            <Label text="URL:" textFill="WHITE">
               <font>
                  <Font name="System Italic" size="12.0" />
               </font>
            </Label>
            <JFXTextField fx:id="URLTXF" focusColor="#00b2ff" focusTraversable="false" prefHeight="25.0" prefWidth="351.0" style="-fx-text-fill: white;" unFocusColor="#ffffffa6">
               <font>
                  <Font name="System Italic" size="12.0" />
               </font>
            </JFXTextField>
         </children>
      </HBox>
      <Label fx:id="LinkID" disable="true" text="Label" visible="false" />
   </children>
</AnchorPane>
