<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane fx:id="Page" prefHeight="200.0" prefWidth="400.0" styleClass="anc" stylesheets="@ChangePasswordPage1.css"
            xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="user.controller.UploadComponent">
   <children>
      <AnchorPane layoutX="93.0" prefHeight="40.0" prefWidth="200.0" styleClass="up" AnchorPane.leftAnchor="0.0"
                  AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <ImageView fx:id="CancelBTN" fitHeight="30.0" fitWidth="30.0" layoutX="356.0" layoutY="5.0"
                       pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../resource/images/icons/icons8_delete_208px.png"/>
               </image>
               <cursor>
                  <Cursor fx:constant="HAND"/>
               </cursor>
            </ImageView>
         </children>
      </AnchorPane>
      <Label alignment="CENTER" layoutX="92.0" layoutY="37.0" prefHeight="50.0" prefWidth="400.0"
             text="Select The Way You Want To Upload:" textFill="WHITE" AnchorPane.leftAnchor="0.0"
             AnchorPane.rightAnchor="0.0">
         <font>
            <Font name="System Italic" size="17.0"/>
         </font>
      </Label>
      <HBox alignment="CENTER" layoutX="72.0" layoutY="122.0" prefHeight="120.0" prefWidth="400.0" spacing="40.0"
            AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <children>
            <Button fx:id="EnterUrlBTN" mnemonicParsing="false" prefHeight="90.0" prefWidth="150.0" styleClass="btn"
                    text="Enter URL" textFill="WHITE">
               <font>
                  <Font name="System Bold Italic" size="15.0"/>
               </font>
            </Button>
            <Button fx:id="SelectBTN" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" prefHeight="90.0"
                    prefWidth="150.0" styleClass="btn" text="Select File" textFill="WHITE">
               <font>
                  <Font name="System Bold Italic" size="15.0"/>
               </font>
            </Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
